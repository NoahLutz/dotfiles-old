#define COLORBG #3b1b2d 
#define COLORFG #DDDDB3
#define COLORBORDER #855e5c


URxvt.scrollBar: false

URxvt*color0: COLOR0
URxvt*color1: COLOR1
URxvt*color2: COLOR2
URxvt*color3: COLOR3
URxvt*color4: COLOR4
URxvt*color5: COLOR5
URxvt*color6: COLOR6
URxvt*color7: COLOR7

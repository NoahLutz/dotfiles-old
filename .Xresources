URxvt*color11: COLOR11
URxvt*color12: COLOR12
URxvt*color13: COLOR13
URxvt*color14: COLOR14
URxvt*color15: COLOR15


!URxvt*.shading: 25
!URxvt*.transparent: true
!URxvt*tintColor: [75]#5e3944

URxvt*foreground: COLORFG
URxvt*background: COLORBG

!Border
URxvt*externalBorder: 0
URxvt*borderColor: COLORBORDER


!Clickable Links
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1

!Copy paste
!URxvt.perl-ext-common: default,clipboard
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:pasteA


!Rofi config

rofi.color-window: COLORBG, COLORBORDER
rofi.color-normal: COLORBG, COLORFG, COLORBG, COLORFG, COLORBG
rofi.color-urgent: COLORBG, COLORFG, COLORBG, COLORFG, COLORBG
rofi.color-active: COLORBG, COLORFG, COLORBG, COLORFG, COLORBG
rofi.separator-style: solid


!Main color scheme

#define COLORBG #323639
#define COLORFG #8E8B86

*color0: #202426
*color1: #555A5E
*color2: #64605D
*color3: #6A5456
*color4: #2E313A
*color5: #494546
*color6: #46495C
*color7: #583A38
*color8: #323639
*color9: #748080
*color10: #7A7677
*color11: #A38583
*color12: #9E7549
*color13: #55585D
*color14: #676C7F
*color15: #845948



!URxvt specific settings

URxvt.scrollBar: false
URxvt*foreground: COLORFG
URxvt*background: COLORBG

!Clickable Links
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/google-chrome-stable
URxvt.matcher.button: 1

!Copy paste
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste


!Rofi config
rofi.color-window: COLORBG, COLORBORDER
rofi.color-normal: COLORBG, COLORFG, COLORBG, COLORFG, COLORBG
rofi.color-urgent: COLORBG, COLORFG, COLORBG, COLORFG, COLORBG
rofi.color-active: COLORBG, COLORFG, COLORBG, COLORFG, COLORBG
rofi.separator-style: solid

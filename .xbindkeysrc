###########################
# xbindkeys configuration #
###########################
#
# Version: 1.8.6

"(amixer set Master 5%+)"
    m:0x0 + c:123
    XF86AudioRaiseVolume

"(amixer set Master 5%-)"
    m:0x0 + c:122
    XF86AudioLowerVolume

"(amixer set Master 1+ toggle)"
    m:0x0 + c:121
    XF86AudioMute

#Rofi Keybinds
"(rofi -modi run -show run -no-fixed-num-lines)"
    m:0x8 + c:65
    Alt + space

"(rofi -modi window -show window -no-fixed-num-lines)"
    m:0x8 + c:23
    Alt + Tab

"(rofi -modi windowcd -show windowcd -no-fixed-num-lines)"
    m:0x8 + c:49
    Alt + grave

"(rofi -modi 'google:/home/noah/bin/google' -show google -no-fixed-num-lines)"
    m:0x9 + c:42
    Shift+Alt + g

"(rofi -modi 'ssh' -show ssh -no-fixed-num-lines)"
    m:0x9 + c:39
    Shift+Alt + s


"(xbacklight -inc 5)"
    m:0x0 + c:233
    XF86MonBrightnessUp

"(xbacklight -dec 5)"
    m:0x0 + c:232
    XF86MonBrightnessDown

##################################
# End of xbindkeys configuration #
##################################
